    "@types/chai": "^4.3.20",
    "@types/node": "^22.14.1",
    "@types/supertest": "^2.0.12",
    "chai": "^4.5.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
